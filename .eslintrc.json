{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "prettier"
    ],
    "plugins": [
        "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "prettier/prettier": "error",
        "no-unused-vars": "warn",
        "no-console": "off"
    },
    "fileInfoOptions": {
        "withNodeModules": true
    }
}